{
	"name": "logos",
	"properties": {
		"activities": [
			{
				"name": "main",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/chandandhonadhi@outlook.com/main"
				},
				"linkedServiceName": {
					"referenceName": "logosdatabricks",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Error_Email",
				"type": "WebHook",
				"dependsOn": [
					{
						"activity": "main",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://logoslogicapp.azurewebsites.net:443",
					"method": "POST",
					"body": {
						"message": "This is a custom dynamic message from your pipeline with run ID @{pipeline().RunId}.",
						"dataFactoryName": "@{pipeline().DataFactory}",
						"pipelineName": "@{pipeline().Pipeline}",
						"receiver": "@{pipeline().parameters.receiver}"
					},
					"timeout": "00:02:00",
					"disableCertValidation": true
				}
			}
		],
		"parameters": {
			"receiver": {
				"type": "string",
				"defaultValue": "surya.5.aari@gmail.com"
			}
		},
		"annotations": []
	}
}